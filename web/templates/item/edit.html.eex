<h2>Edit <%= @item.name %></h2>

<%=
  render(
    "form.html",
    conn: @conn,
    users: @users,
    changeset: @changeset,
    submit_path: list_item_path(@conn, :update, @item.list, @item),
    submit_label: "Update!"
  )
%>

<%= link "Cancel", to: list_item_path(@conn, :show, @item.list, @item) %>
